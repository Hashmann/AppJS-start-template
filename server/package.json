{
  "name": "server",
  "version": "1.0.0",
  "description": "Express server template for a quick start of development",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV= nodemon server.js",
    "seed": "cross-env NODE_ENV= node config/db.seeder.js",
    "start": "cross-env NODE_ENV=.production node server.js",
    "start:dev": "cross-env NODE_ENV=.development nodemon server.js",
    "test": "cross-env NODE_ENV= jest"
  },
  "keywords": [
    "mongo",
    "node",
    "express",
    "socket.io"
  ],
  "author": "Yarkov Vyacheslav <yarkov.slav@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "5.1.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-validator": "6.14.3",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongodb": "4.13.0",
    "mongoose": "6.9.0",
    "nodemailer": "6.9.1",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@faker-js/faker": "^7.6.0",
    "@shelf/jest-mongodb": "^4.1.6",
    "babel-jest": "^29.4.2",
    "cross-env": "^7.0.3",
    "jest": "^29.4.2",
    "nodemon": "2.0.20",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.6.0"
  }
}
